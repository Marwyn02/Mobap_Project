{
  "expo": {
    "name": "Mobap",
    "slug": "code-crusaders",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.earist.codecrusaders"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.earist.codecrusaders"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "scheme": "your-app-scheme",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "fd58ffab-9330-4795-9f86-08f233e75821"
      }
    },
    "owner": "marwyn02"
  }
}
